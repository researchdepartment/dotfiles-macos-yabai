Match originalhost proxmox-root exec "[ $(/System/Library/PrivateFrameworks/Apple80211.framework/Versions/Current/Resources/airport -I | awk '/ SSID/ {print substr($0, index($0, $2))}') != DataMiners5G ]"
    User root
    HostName proxmox-host.local
    Port 2244

Host proxmox-root
    HostName pi.local
    User root
    Port 2245

Match originalhost pi-root exec "[ $(/System/Library/PrivateFrameworks/Apple80211.framework/Versions/Current/Resources/airport -I | awk '/ SSID/ {print substr($0, index($0, $2))}') != DataMiners5G ]"
    User root
    HostName pi-host.local
    Port 2244

Host pi-root
    User root
    HostName pi.local
    Port 2244

Match originalhost pi-keira exec "[ $(/System/Library/PrivateFrameworks/Apple80211.framework/Versions/Current/Resources/airport -I | awk '/ SSID/ {print substr($0, index($0, $2))}') != DataMiners5G ]"
    User keira
    HostName pi-host.local
    Port 2244

Host pi-keira
    User keira
    HostName pi.local
    Port 2244

Host *
    ServerAliveInterval 25
    ServerAliveCountMax 3
    VisualHostKey true
